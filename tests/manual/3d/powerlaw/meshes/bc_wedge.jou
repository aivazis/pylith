# ----------------------------------------------------------------------
# Create blocks for materials
# ----------------------------------------------------------------------
block 1 volume all
block 1 name "viscoelastic"

# ----------------------------------------------------------------------
# Create nodeset for outer surface
# ----------------------------------------------------------------------
group "boundary_outer" add node in surface 15
nodeset 21 group boundary_outer
nodeset 21 name "boundary_outer"

# ----------------------------------------------------------------------
# Create nodeset for outer surface with edge nodes removed
# ----------------------------------------------------------------------
group "boundary_outer_restricted" add node in surface 15
group "boundary_outer_restricted" remove node in surface 17 14
nodeset 27 group boundary_outer_restricted
nodeset 27 name "boundary_outer_restricted"

# ----------------------------------------------------------------------
# Create nodeset for -x boundary
# ----------------------------------------------------------------------
group "boundary_xneg" add node in surface 14
nodeset 22 group boundary_xneg
nodeset 22 name "boundary_xneg"

# ----------------------------------------------------------------------
# Create nodeset for -y boundary
# ----------------------------------------------------------------------
group "boundary_yneg" add node in surface 17
nodeset 23 group boundary_yneg
nodeset 23 name "boundary_yneg"

# ----------------------------------------------------------------------
# Create nodeset for +z boundary
# ----------------------------------------------------------------------
group "boundary_zpos" add node in surface 16
nodeset 24 group boundary_zpos
nodeset 24 name "boundary_zpos"

# ----------------------------------------------------------------------
# Create nodeset for -z boundary
# ----------------------------------------------------------------------
group "boundary_zneg" add node in surface 18
nodeset 25 group boundary_zneg
nodeset 25 name "boundary_zneg"


